{
  "expo": {
    "name": "Buyzaar",
    "slug": "buyzaar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "buyzaar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF8E31"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hamzaamjd.buyzaar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF8E31"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AlzaSyIUN69PX-YVxqtU4Oeq0XjZtW-h0cUZ3de"
        }
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.hamzaamjd.buyzaar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0e8e42fe-dc7d-4ce5-8315-d9440e3561fb"
      }
    }
  }
}
